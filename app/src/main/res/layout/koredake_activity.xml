<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".Koredake">

    <com.dotinstall.secaiderf2.KoreHealthBack
        android:id="@+id/back"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <ImageView
        android:id="@+id/secListView"
        android:layout_width="350dp"
        android:layout_height="482dp"
        android:layout_marginTop="52dp"
        android:layout_below="@+id/hIcon"
        android:layout_centerHorizontal="true" />

    <ImageView
        android:id="@+id/sanIcon"
        android:layout_width="50dp"
        android:layout_height="43dp"
        android:layout_alignTop="@+id/biIcon"
        android:layout_toStartOf="@+id/biIcon"
        android:layout_marginEnd="14dp" />

    <ImageView
        android:id="@+id/biIcon"
        android:layout_width="50dp"
        android:layout_height="43dp"
        android:layout_marginEnd="11dp"
        android:layout_alignTop="@+id/hIcon"
        android:layout_toStartOf="@+id/hIcon"
        android:onClick="main_view" />

    <ImageView
        android:id="@+id/hIcon"
        android:layout_width="50dp"
        android:layout_height="43dp"
        android:onClick="health_data"
        android:layout_marginTop="42dp"
        android:layout_alignParentTop="true"
        android:layout_alignEnd="@+id/secListView" />

    <ImageView
        android:id="@+id/korenara"
        android:layout_width="150dp"
        android:layout_height="159dp"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/secListView"
        android:layout_marginTop="14dp" />

    <LinearLayout
        android:layout_width="330dp"
        android:layout_height="400dp"
        android:orientation="vertical"
        android:layout_alignBottom="@+id/secListView"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="18dp">

    <ImageView
        android:id="@+id/push"
        android:layout_width="match_parent"
        android:layout_height="43dp"
        android:layout_above="@+id/koredakeView"
        android:layout_alignStart="@+id/koredakeView"
        android:layout_alignEnd="@+id/koredakeView" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="50dp" />

    <ListView
        android:id="@+id/koredakeView"
        android:layout_width="301dp"
        android:layout_height="322dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.507"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        app:layout_constraintVertical_bias="0.15"
        android:layout_alignBottom="@+id/secListView"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="46dp" />

    </LinearLayout>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="30dp"
        android:text=""
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:textSize="15sp"
        android:onClick="health_data" />


</RelativeLayout>